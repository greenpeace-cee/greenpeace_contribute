<?xml version="1.0"?>
<extension key="greenpeace_contribute" type="module">
  <file>greenpeace_contribute</file>
  <name>Greenpeace Contribute</name>
  <description>Managed entities for contribution-related CustomFields</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Manuel Flandorfer</author>
    <email>mflandor@greenpeace.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/greenpeace-cee/greenpeace_contribute</url>
    <url desc="Documentation">https://github.com/greenpeace-cee/greenpeace_contribute</url>
    <url desc="Support">https://github.com/greenpeace-cee/greenpeace_contribute/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-09-05</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.69</ver>
  </compatibility>
  <requires>
    <ext>org.project60.banking</ext>
  </requires>
  <comments></comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/GreenpeaceContribute</namespace>
    <format>23.02.1</format>
    <angularModule>crmGreenpeaceContribute</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
</extension>
